{
  "name": "fahndung",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "next lint && tsc --noEmit",
    "dev": "next dev",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "format:write": "prettier --write \"**/*.{ts,tsx,js,jsx,mdx}\" --cache",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "setup:remote": "bash scripts/setup-remote-supabase.sh setup",
    "test:remote": "bash scripts/setup-remote-supabase.sh test",
    "design:tokens:test": "node scripts/test-design-tokens.js",
    "design:tokens:migrate": "node scripts/migrate-to-design-tokens.js",
    "design:tokens:migrate:dry": "node scripts/migrate-to-design-tokens.js --dry-run",
    "clean": "rimraf .next dist build coverage node_modules tsconfig.tsbuildinfo",
    "fresh": "pnpm clean && pnpm install",
    "clean:build": "rimraf .next dist build coverage",
    "clean:deps": "pnpm prune && pnpm dedupe",
    "clean:store": "pnpm store prune",
    "fix:all": "pnpm fix:types && pnpm fix:nullish && pnpm lint:fix && pnpm format:write",
    "fix:types": "find src -type f \\( -name '*.ts' -o -name '*.tsx' \\) -exec sed -i 's/: any/: unknown/g' {} +",
    "fix:nullish": "find src -type f \\( -name '*.ts' -o -name '*.tsx' \\) -exec sed -i 's/\\([a-zA-Z_][a-zA-Z0-9_]*\\) || \\(null\\|undefined\\)/\\1 ?? \\2/g' {} +",
    "doctor": "pnpm typecheck || pnpm fix:all && pnpm build",
    "health": "pnpm health:fix && pnpm health:check && pnpm health:report",
    "health:fix": "sed -i 's/!\\([a-zA-Z_]*\\) ?? !\\([a-zA-Z_]*\\)/!\\1 || !\\2/g' src/**/*.{ts,tsx}",
    "health:check": "pnpm typecheck && pnpm lint && pnpm audit --prod",
    "health:report": "pnpm outdated && echo 'âœ… System Health Check Complete'",
    "build:force": "SKIP_LINTING=true next build || echo 'Build completed with warnings'"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@supabase/supabase-js": "^2.52.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.1",
    "lucide-react": "0.525.0",
    "next": "^15.2.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "server-only": "^0.0.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.24.2",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@lhci/cli": "^0.15.1",
    "@next/bundle-analyzer": "^15.4.6",
    "@tailwindcss/postcss": "^4.1.11",
    "@types/node": "^20.14.10",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.23.0",
    "eslint-config-next": "^15.2.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.27.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9.0.0"
  },
  "ct3aMetadata": {
    "initVersion": "7.39.3"
  },
  "packageManager": "pnpm@10.14.0"
}
